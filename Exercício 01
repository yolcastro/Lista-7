#include <stdio.h>

struct ALUNO {
    float n1;
    float n2;
    float media;
};

int main() {
    struct ALUNO aluno;

    printf("Digite a primeira nota: ");
    scanf("%f", &aluno.n1);
    printf("Digite a segunda nota: ");
    scanf("%f", &aluno.n2);

    aluno.media = (aluno.n1 + aluno.n2) / 2;

    printf("Nota 1: %.2f\n", aluno.n1);
    printf("Nota 2: %.2f\n", aluno.n2);
    printf("MÃ©dia: %.2f\n", aluno.media);

    return 0;
}
